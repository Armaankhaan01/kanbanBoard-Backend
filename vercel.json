{
    "version": 1,
    "builds": [
      { "src": "index.js", "use": "@vercel/node" },
      { "src": "public/**", "use": "@vercel/static" }
    ],
    "routes": [
      {"handle": "filesystem"},
      { "src": "/(.*)", "dest": "/index.js" },
      { "src": "/css/(.*)", "dest": "/public/css/$1" }
    ]
  }